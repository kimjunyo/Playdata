 반복문
 for( ; ; ){
 }
 for(int n=0;n<10;n++){
 }
 for(int n=10;n>0;n--){
 }
 for(하나의 값을 저장하는 변수:다중값변수){ //다중값 변수에는 배열 가능 //배열에서 모든 것을 다 꺼내야 for문 종료
 } //for-in 문
 
 while(조건){
 	실행문
 	while을 빠져나가기 위한 증감.
 }
 예)
 int n=0;
 while(n<3){
 	System.out.println(n);
 	n++;
 	
 }
 boolean b=true;
 while(b){
 	b=false;
 }